{
  "mcpProxy": {
    "baseURL": "http://localhost:6277",
    "addr": ":6277",
    "name": "MCP Proxy",
    "version": "1.0.0",
    "options": {
      "panicIfInvalid": false,
      "logEnabled": true,
      "authTokens": []
    }
  },
  "mcpServers": {
    "fetch": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ],
      "env": {},
      "options": {
        "panicIfInvalid": true,
        "logEnabled": true,
        "authTokens": []
      }
    },
    "tavily-mcp-sse": {
      "url": "https://xxx.api-inference.modelscope.cn/sse"
    },
    "tavily-mcp-streamable": {
      "transportType": "streamable-http",
      "url": "https://router.mcp.so/mcp/xxx"
    }
  }
}